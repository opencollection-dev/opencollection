{
  "name": "@opencollection/types",
  "version": "0.4.0",
  "description": "TypeScript types for OpenCollection schema",
  "author": "Bruno Software Inc.",
  "main": "dist/opencollection.js",
  "types": "dist/opencollection.d.ts",
  "files": [
    "dist",
    "src",
    "package.json"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/opencollection.d.ts",
      "default": "./dist/opencollection.js"
    },
    "./common/*": {
      "types": "./dist/common/*.d.ts",
      "default": "./dist/common/*.js"
    },
    "./config/*": {
      "types": "./dist/config/*.d.ts",
      "default": "./dist/config/*.js"
    },
    "./collection/*": {
      "types": "./dist/collection/*.d.ts",
      "default": "./dist/collection/*.js"
    },
    "./requests/*": {
      "types": "./dist/requests/*.d.ts",
      "default": "./dist/requests/*.js"
    }
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "license": "MIT",
  "keywords": [
    "opencollection",
    "types",
    "typescript",
    "api",
    "http"
  ]
}

